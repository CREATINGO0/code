import java.util.*;

public class DateTester {
    public static void main(String[] args) {
          	
    	 String mon = new String("Apr");
    	 int m;
    	 switch(mon) {
    		case "Jan":
    			m = 0;
    			break;

    		case "Feb":
    			m = 1;
    			break;
    		
    		case "Mar":
    			m = 2;
    			break;
    		
    		case "Apr":
    			m = 3;
    			break;
    		
    		case "May":
    			m = 4;
    			break;
    		
    		case "Jun":
    			m = 5;
    			break;

    		default:
    			m = 6;
    			break;
    	}
    	 System.out.println(m);
    	 
